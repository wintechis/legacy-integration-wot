    <!-- https://purl.org/sco/ns#dataModelOf -->


    <owl:ObjectProperty rdf:about="https://purl.org/sco/ns#dataModelOf">
        <owl:inverseOf rdf:resource="https://purl.org/sco/ns#hasDataModel"/>
        <rdfs:domain rdf:resource="https://purl.org/sco/ns#DataModel"/>
        <rdfs:range rdf:resource="https://purl.org/sco/ns#ServiceCapability"/>
        <rdfs:comment>Represents the connection of data model to a specific service capability.</rdfs:comment>
        <rdfs:label>is datamodel of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/sco/ns#decodingRuleOf -->


    <owl:ObjectProperty rdf:about="https://purl.org/sco/ns#decodingRuleOf">
        <rdfs:subPropertyOf rdf:resource="https://purl.org/sco/ns#transformationRuleOf"/>
        <owl:inverseOf rdf:resource="https://purl.org/sco/ns#hasDecodingRule"/>
        <rdfs:domain rdf:resource="https://purl.org/sco/ns#TransformationRule"/>
        <rdfs:range rdf:resource="https://purl.org/sco/ns#DataModel"/>
        <rdfs:comment>Is the decoding rule (a specail type of transformation rule) for some data model.</rdfs:comment>
        <rdfs:label>is decoding rule of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/sco/ns#encodingRuleOf -->


    <owl:ObjectProperty rdf:about="https://purl.org/sco/ns#encodingRuleOf">
        <rdfs:subPropertyOf rdf:resource="https://purl.org/sco/ns#transformationRuleOf"/>
        <owl:inverseOf rdf:resource="https://purl.org/sco/ns#hasEncodingRule"/>
        <rdfs:domain rdf:resource="https://purl.org/sco/ns#TransformationRule"/>
        <rdfs:range rdf:resource="https://purl.org/sco/ns#DataModel"/>
        <rdfs:comment>Is the encoding rule (a specail type of transformation rule) for some data model.</rdfs:comment>
        <rdfs:label>is encoding rule of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/sco/ns#hasDataModel -->


    <owl:ObjectProperty rdf:about="https://purl.org/sco/ns#hasDataModel">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://purl.org/sco/ns#ServiceCapability"/>
        <rdfs:range rdf:resource="https://purl.org/sco/ns#DataModel"/>
        <rdfs:comment>References to the data model of a service capability of a device.</rdfs:comment>
        <rdfs:label>has datamodel</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/sco/ns#hasDecodingRule -->


    <owl:ObjectProperty rdf:about="https://purl.org/sco/ns#hasDecodingRule">
        <rdfs:subPropertyOf rdf:resource="https://purl.org/sco/ns#hasTransformationRule"/>
        <rdfs:domain rdf:resource="https://purl.org/sco/ns#DataModel"/>
        <rdfs:range rdf:resource="https://purl.org/sco/ns#TransformationRule"/>
        <rdfs:comment>Identifies the transformation rule as a decoding rule, to decode some data provided by the device.</rdfs:comment>
        <rdfs:label>has decoding rule</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/sco/ns#hasEncodingRule -->


    <owl:ObjectProperty rdf:about="https://purl.org/sco/ns#hasEncodingRule">
        <rdfs:subPropertyOf rdf:resource="https://purl.org/sco/ns#hasTransformationRule"/>
        <rdfs:domain rdf:resource="https://purl.org/sco/ns#DataModel"/>
        <rdfs:range rdf:resource="https://purl.org/sco/ns#TransformationRule"/>
        <rdfs:comment>Identifies the transformation rule as a encoding rule, to encode some data provided for the device.</rdfs:comment>
        <rdfs:label>has encoding rule</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/sco/ns#hasInputFormat -->


    <owl:ObjectProperty rdf:about="https://purl.org/sco/ns#hasInputFormat">
        <owl:inverseOf rdf:resource="https://purl.org/sco/ns#inputFormatOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://purl.org/sco/ns#DataModel"/>
        <rdfs:range rdf:resource="https://purl.org/sco/ns#Format"/>
        <rdfs:comment>Defines the expected inputformat for some data of a service capability.</rdfs:comment>
        <rdfs:label>has inputformat</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/sco/ns#hasInteraction -->


    <owl:ObjectProperty rdf:about="https://purl.org/sco/ns#hasInteraction">
        <owl:inverseOf rdf:resource="https://purl.org/sco/ns#interactionOf"/>
        <rdfs:domain rdf:resource="https://purl.org/sco/ns#ServiceCapability"/>
        <rdfs:range rdf:resource="https://purl.org/sco/ns#ServiceInteraction"/>
        <rdfs:comment>Indicates the interaction provided by a service capability through a specific service interaction method.</rdfs:comment>
        <rdfs:label>has interaction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/sco/ns#hasManufacturer -->


    <owl:ObjectProperty rdf:about="https://purl.org/sco/ns#hasManufacturer">
        <owl:inverseOf rdf:resource="https://purl.org/sco/ns#manufacturerOf"/>
        <rdfs:domain rdf:resource="https://purl.org/sco/ns#Device"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/sco/ns#hasOutputFormat -->


    <owl:ObjectProperty rdf:about="https://purl.org/sco/ns#hasOutputFormat">
        <owl:inverseOf rdf:resource="https://purl.org/sco/ns#outputFormatOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://purl.org/sco/ns#DataModel"/>
        <rdfs:range rdf:resource="https://purl.org/sco/ns#Format"/>
        <rdfs:comment>Defines the expected outputformat for some data of a service capability.</rdfs:comment>
        <rdfs:label>has outputformat</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/sco/ns#hasProtocolStack -->


    <owl:ObjectProperty rdf:about="https://purl.org/sco/ns#hasProtocolStack">
        <owl:inverseOf rdf:resource="https://purl.org/sco/ns#protocolStackOf"/>
        <rdfs:domain rdf:resource="https://purl.org/sco/ns#ServiceInteraction"/>
        <rdfs:range rdf:resource="https://purl.org/sco/ns#ProtocolStack"/>
        <rdfs:comment>Refers to the set of network protocols used by a device or service.</rdfs:comment>
        <rdfs:label>has protocolstack</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/sco/ns#hasServiceCapability -->


    <owl:ObjectProperty rdf:about="https://purl.org/sco/ns#hasServiceCapability">
        <owl:inverseOf rdf:resource="https://purl.org/sco/ns#serviceCapabilityOf"/>
        <rdfs:domain rdf:resource="https://purl.org/sco/ns#Device"/>
        <rdfs:range rdf:resource="https://purl.org/sco/ns#ServiceCapability"/>
        <rdfs:comment>Highlights the relation between the service capability and the device.</rdfs:comment>
        <rdfs:label>has servicecapability</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/sco/ns#hasTransformationRule -->


    <owl:ObjectProperty rdf:about="https://purl.org/sco/ns#hasTransformationRule">
        <owl:inverseOf rdf:resource="https://purl.org/sco/ns#transformationRuleOf"/>
        <rdfs:domain rdf:resource="https://purl.org/sco/ns#DataModel"/>
        <rdfs:range rdf:resource="https://purl.org/sco/ns#TransformationRule"/>
        <rdfs:comment>The rule used on some data provided through the data model.</rdfs:comment>
        <rdfs:label>has transformation rule</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/sco/ns#hasUnit -->


    <owl:ObjectProperty rdf:about="https://purl.org/sco/ns#hasUnit">
        <owl:inverseOf rdf:resource="https://purl.org/sco/ns#unitOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://purl.org/sco/ns#DataModel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>Links the unit of some data provided by the device to the data model.</rdfs:comment>
        <rdfs:label>has unit</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/sco/ns#inputFormatOf -->


    <owl:ObjectProperty rdf:about="https://purl.org/sco/ns#inputFormatOf">
        <rdfs:domain rdf:resource="https://purl.org/sco/ns#Format"/>
        <rdfs:range rdf:resource="https://purl.org/sco/ns#DataModel"/>
        <rdfs:comment>Is the expected inputformat from the device after executing the optionally transformation function.</rdfs:comment>
        <rdfs:label>is inputformat of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/sco/ns#interactionOf -->


    <owl:ObjectProperty rdf:about="https://purl.org/sco/ns#interactionOf">
        <rdfs:domain rdf:resource="https://purl.org/sco/ns#ServiceInteraction"/>
        <rdfs:range rdf:resource="https://purl.org/sco/ns#ServiceCapability"/>
        <rdfs:comment>References to a service interaction that is part of a specific service capability.</rdfs:comment>
        <rdfs:label>is interaction of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/sco/ns#manufacturerOf -->


    <owl:ObjectProperty rdf:about="https://purl.org/sco/ns#manufacturerOf">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://purl.org/sco/ns#Device"/>
        <rdfs:comment>Links the manufacturer to a specifc device.</rdfs:comment>
        <rdfs:label>is manufacturer of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/sco/ns#outputFormatOf -->


    <owl:ObjectProperty rdf:about="https://purl.org/sco/ns#outputFormatOf">
        <rdfs:domain rdf:resource="https://purl.org/sco/ns#Format"/>
        <rdfs:range rdf:resource="https://purl.org/sco/ns#DataModel"/>
        <rdfs:comment>Is the expected outputformat from the device after executing the optionally transformation function.</rdfs:comment>
        <rdfs:label>is output format of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/sco/ns#protocolStackOf -->


    <owl:ObjectProperty rdf:about="https://purl.org/sco/ns#protocolStackOf">
        <rdfs:domain rdf:resource="https://purl.org/sco/ns#ProtocolStack"/>
        <rdfs:range rdf:resource="https://purl.org/sco/ns#ServiceInteraction"/>
        <rdfs:comment>Is the protocol stack used for some interaction with a service capability.</rdfs:comment>
        <rdfs:label>is protocolstack of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/sco/ns#serviceCapabilityOf -->


    <owl:ObjectProperty rdf:about="https://purl.org/sco/ns#serviceCapabilityOf">
        <rdfs:domain rdf:resource="https://purl.org/sco/ns#ServiceCapability"/>
        <rdfs:range rdf:resource="https://purl.org/sco/ns#Device"/>
        <rdfs:comment>Is the service capability of a device.</rdfs:comment>
        <rdfs:label>is servicecapability of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/sco/ns#transformationRuleOf -->


    <owl:ObjectProperty rdf:about="https://purl.org/sco/ns#transformationRuleOf">
        <rdfs:domain rdf:resource="https://purl.org/sco/ns#TransformationRule"/>
        <rdfs:range rdf:resource="https://purl.org/sco/ns#DataModel"/>
        <rdfs:comment>Is the transformation rule for some data model.</rdfs:comment>
        <rdfs:label>is transformationrule of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/sco/ns#unitOf -->


    <owl:ObjectProperty rdf:about="https://purl.org/sco/ns#unitOf">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://purl.org/sco/ns#DataModel"/>
        <rdfs:comment>A unit of a data model.</rdfs:comment>
        <rdfs:label>is unit of</rdfs:label>
    </owl:ObjectProperty>
    


