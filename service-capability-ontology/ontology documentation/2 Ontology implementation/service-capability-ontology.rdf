    <owl:Ontology rdf:about="https://purl.org/sco/ns#">
        <owl:versionIRI rdf:resource="https://purl.org/sco/ns#"/>
        <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#gYear">2024</terms:created>
        <terms:creator>
            <rdf:Description>
                <schema:affiliation>
                    <rdf:Description>
                        <foaf:homepage rdf:resource="https://www.scs.fraunhofer.de/de/ueber-uns/organisation/data-spaces-and-iot-solutions.html"/>
                        <foaf:name>Data Spaces and IoT Solutions</foaf:name>
                    </rdf:Description>
                </schema:affiliation>
                <rdfs:seeAlso rdf:resource="https://renedorsch.solidweb.org/#me"/>
                <foaf:homepage rdf:resource="https://orcid.org/0000-0001-6857-7314"/>
                <foaf:name>Rene Dorsch</foaf:name>
            </rdf:Description>
        </terms:creator>
        <terms:description xml:lang="en">This is the Service Capability (SCO). SCO is a light-weight, application ontology for the translation of legacy IoT devices to the W3C Web of Things. SCO contributes to the automatic generation of Thing Descriptions for modeling the key components necessary for a W3C Web of Things Thing Description.  </terms:description>
        <terms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-01-14</terms:issued>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by-sa/4.0/"/>
        <terms:publisher rdf:resource="https://renedorsch.solidweb.org/#me"/>
        <terms:contributor>
            <rdf:Description>
                <schema:affiliation>
                    <rdf:Description>
                        <foaf:homepage rdf:resource="https://www.th-nuernberg.de/einrichtungen-gesamt/fraunhofer-forschungsgruppen/future-engineering/"/>
                        <foaf:name>Future Engineering Research Group</foaf:name>
                    </rdf:Description>
                </schema:affiliation>
                <rdfs:seeAlso rdf:resource="https://www.th-nuernberg.de/person/ascencion-arevalo-kiara-marnitt/"/>
                <foaf:homepage rdf:resource="https://orcid.org/0000-0002-4158-9478"/>
                <foaf:name>Kiara Marnitt Ascencion Arevalo</foaf:name>
            </rdf:Description>
        </terms:contributor>
        <terms:title xml:lang="en">Service Capability Ontology</terms:title>
        <bibo:doi rdf:resource="https://doi.org/tbd"/>
        <vann:preferredNamespacePrefix>sco</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceURI>https://purl.org/sco/ns#</vann:preferredNamespaceURI>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">1.0</owl:versionInfo>
        <foaf:depiction rdf:resource="http://purl.org/sco/schema_diagram"/>
        <ns:license rdf:resource="https://creativecommons.org/licenses/by-sa/4.0/"/>
        <mod:acronym>SCO</mod:acronym>
        <mod:competencyQuestion rdf:resource="http://purl.org/sco/repository/cq"/>
        <mod:hasSyntax rdf:resource="http://www.w3.org/ns/formats/Turtle"/>
    </owl:Ontology>



