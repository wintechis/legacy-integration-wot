    <!-- https://purl.org/sco/ns#hasAddress -->


    <owl:DatatypeProperty rdf:about="https://purl.org/sco/ns#hasAddress">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://purl.org/sco/ns#ServiceInteraction"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The address of a service capability to interact with it. E.g. a href for HTTP.</rdfs:comment>
        <rdfs:label>has Address</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://purl.org/sco/ns#hasManufacturer -->


    <owl:DatatypeProperty rdf:about="https://purl.org/sco/ns#hasManufacturer"/>
    


    <!-- https://purl.org/sco/ns#hasName -->


    <owl:DatatypeProperty rdf:about="https://purl.org/sco/ns#hasName">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://purl.org/sco/ns#Device"/>
                    <rdf:Description rdf:about="https://purl.org/sco/ns#ServiceCapability"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The name of a device. Often a device provides a prefered name (e.g. Bluetooth LE or ZigBee).</rdfs:comment>
        <rdfs:label>has Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://purl.org/sco/ns#ActionInteraction -->


    <owl:Class rdf:about="https://purl.org/sco/ns#ActionInteraction">
        <rdfs:subClassOf rdf:resource="https://purl.org/sco/ns#ServiceInteraction"/>
        <rdfs:comment>A action that is triggered whenever a service capability from the device is triggered. A action is a method to mainpultae or trigger a process.</rdfs:comment>
        <rdfs:label>Action Interaction</rdfs:label>
    </owl:Class>
    


    <!-- https://purl.org/sco/ns#DataModel -->


    <owl:Class rdf:about="https://purl.org/sco/ns#DataModel">
        <rdfs:comment>The Data Model describes how data for a specific Service Capability for the exchange between the device and another device is prepared.</rdfs:comment>
        <rdfs:label>Data Model</rdfs:label>
    </owl:Class>
    


    <!-- https://purl.org/sco/ns#Device -->


    <owl:Class rdf:about="https://purl.org/sco/ns#Device">
        <rdfs:comment>A device, capable of machine-to-machine (M2M) communication.</rdfs:comment>
        <rdfs:label>Device</rdfs:label>
    </owl:Class>
    


    <!-- https://purl.org/sco/ns#Format -->


    <owl:Class rdf:about="https://purl.org/sco/ns#Format">
        <rdfs:comment>A kind of dataformat to interpret the data exchanged by the Service capability.</rdfs:comment>
        <rdfs:label>Format</rdfs:label>
    </owl:Class>
    


    <!-- https://purl.org/sco/ns#ProtocolStack -->


    <owl:Class rdf:about="https://purl.org/sco/ns#ProtocolStack">
        <rdfs:comment>The machine-to-machine communication protocol used to exchange information between two devices.</rdfs:comment>
        <rdfs:label>Protocolstack</rdfs:label>
    </owl:Class>
    


    <!-- https://purl.org/sco/ns#ReadInteraction -->


    <owl:Class rdf:about="https://purl.org/sco/ns#ReadInteraction">
        <rdfs:subClassOf rdf:resource="https://purl.org/sco/ns#ServiceInteraction"/>
        <rdfs:comment>A reading of some data from the device whenever a service capability is triggered.</rdfs:comment>
        <rdfs:label>Read Interaction</rdfs:label>
    </owl:Class>
    


    <!-- https://purl.org/sco/ns#ServiceCapability -->


    <owl:Class rdf:about="https://purl.org/sco/ns#ServiceCapability">
        <rdfs:comment>A service provided by a device to achieve some kind of processing. The service has often in machine-to-machine communication protocols other names, such as Characteristic in Bluetooth LE, Command or Attribute in ZigBee.</rdfs:comment>
        <rdfs:label>Service Capability</rdfs:label>
    </owl:Class>
    


    <!-- https://purl.org/sco/ns#ServiceInteraction -->


    <owl:Class rdf:about="https://purl.org/sco/ns#ServiceInteraction">
        <rdfs:comment>A kind of interaction provided through the service capability from the device.</rdfs:comment>
        <rdfs:label>Service Interaction</rdfs:label>
    </owl:Class>
    


    <!-- https://purl.org/sco/ns#SubscribeInteraction -->


    <owl:Class rdf:about="https://purl.org/sco/ns#SubscribeInteraction">
        <rdfs:subClassOf rdf:resource="https://purl.org/sco/ns#ServiceInteraction"/>
