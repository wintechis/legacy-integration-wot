###  http://purl.org/sco/ns#dataModelOf
sco:dataModelOf rdf:type owl:ObjectProperty ;
                owl:inverseOf sco:hasDataModel ;
                rdfs:domain sco:DataModel ;
                rdfs:range sco:ServiceCapability ;
                rdfs:comment "Represents the connection of data model to a specific service capability." ;
                rdfs:label "is datamodel of" .


###  http://purl.org/sco/ns#decodingRuleOf
sco:decodingRuleOf rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf sco:transformationRuleOf ;
                   owl:inverseOf sco:hasDecodingRule ;
                   rdfs:domain sco:TransformationRule ;
                   rdfs:range sco:DataModel ;
                   rdfs:comment "Is the decoding rule (a specail type of transformation rule) for some data model." ;
                   rdfs:label "is decoding rule of" .


###  http://purl.org/sco/ns#encodingRuleOf
sco:encodingRuleOf rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf sco:transformationRuleOf ;
                   owl:inverseOf sco:hasEncodingRule ;
                   rdfs:domain sco:TransformationRule ;
                   rdfs:range sco:DataModel ;
                   rdfs:comment "Is the encoding rule (a specail type of transformation rule) for some data model." ;
                   rdfs:label "is encoding rule of" .


###  http://purl.org/sco/ns#hasDataModel
sco:hasDataModel rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain sco:ServiceCapability ;
                 rdfs:range sco:DataModel ;
                 rdfs:comment "References to the data model of a service capability of a device." ;
                 rdfs:label "has datamodel" .


###  http://purl.org/sco/ns#hasDecodingRule
sco:hasDecodingRule rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf sco:hasTransformationRule ;
                    rdfs:domain sco:DataModel ;
                    rdfs:range sco:TransformationRule ;
                    rdfs:comment "Identifies the transformation rule as a decoding rule, to decode some data provided by the device." ;
                    rdfs:label "has decoding rule" .


###  http://purl.org/sco/ns#hasEncodingRule
sco:hasEncodingRule rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf sco:hasTransformationRule ;
                    rdfs:domain sco:DataModel ;
                    rdfs:range sco:TransformationRule ;
                    rdfs:comment "Identifies the transformation rule as a encoding rule, to encode some data provided for the device." ;
                    rdfs:label "has encoding rule" .


###  http://purl.org/sco/ns#hasInputFormat
sco:hasInputFormat rdf:type owl:ObjectProperty ;
                   owl:inverseOf sco:inputFormatOf ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain sco:DataModel ;
                   rdfs:range sco:Format ;
                   rdfs:comment "Defines the expected inputformat for some data of a service capability." ;
                   rdfs:label "has inputformat" .


###  http://purl.org/sco/ns#hasInteraction
sco:hasInteraction rdf:type owl:ObjectProperty ;
                   owl:inverseOf sco:interactionOf ;
                   rdfs:domain sco:ServiceCapability ;
                   rdfs:range sco:ServiceInteraction ;
                   rdfs:comment "Indicates the interaction provided by a service capability through a specific service interaction method." ;
                   rdfs:label "has interaction" .


###  http://purl.org/sco/ns#hasManufacturer
sco:hasManufacturer rdf:type owl:ObjectProperty ;
                    owl:inverseOf sco:manufacturerOf ;
                    rdfs:domain sco:Device ;
                    rdfs:range owl:Thing .


###  http://purl.org/sco/ns#hasOutputFormat
sco:hasOutputFormat rdf:type owl:ObjectProperty ;
                    owl:inverseOf sco:outputFormatOf ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:domain sco:DataModel ;
                    rdfs:range sco:Format ;
                    rdfs:comment "Defines the expected outputformat for some data of a service capability." ;
                    rdfs:label "has outputformat" .


###  http://purl.org/sco/ns#hasProtocolStack
sco:hasProtocolStack rdf:type owl:ObjectProperty ;
                     owl:inverseOf sco:protocolStackOf ;
                     rdfs:domain sco:ServiceInteraction ;
                     rdfs:range sco:ProtocolStack ;
                     rdfs:comment "Refers to the set of network protocols used by a device or service." ;
                     rdfs:label "has protocolstack" .


###  http://purl.org/sco/ns#hasServiceCapability
sco:hasServiceCapability rdf:type owl:ObjectProperty ;
                         owl:inverseOf sco:serviceCapabilityOf ;
                         rdfs:domain sco:Device ;
                         rdfs:range sco:ServiceCapability ;
                         rdfs:comment "Highlights the relation between the service capability and the device." ;
                         rdfs:label "has servicecapability" .


###  http://purl.org/sco/ns#hasTransformationRule
sco:hasTransformationRule rdf:type owl:ObjectProperty ;
                          owl:inverseOf sco:transformationRuleOf ;
                          rdfs:domain sco:DataModel ;
                          rdfs:range sco:TransformationRule ;
                          rdfs:comment "The rule used on some data provided through the data model." ;
                          rdfs:label "has transformation rule" .


###  http://purl.org/sco/ns#hasUnit
sco:hasUnit rdf:type owl:ObjectProperty ;
            owl:inverseOf sco:unitOf ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain sco:DataModel ;
            rdfs:range owl:Thing ;
            rdfs:comment "Links the unit of some data provided by the device to the data model." ;
            rdfs:label "has unit" .


###  http://purl.org/sco/ns#inputFormatOf
sco:inputFormatOf rdf:type owl:ObjectProperty ;
                  rdfs:domain sco:Format ;
                  rdfs:range sco:DataModel ;
                  rdfs:comment "Is the expected inputformat from the device after executing the optionally transformation function." ;
                  rdfs:label "is inputformat of" .


###  http://purl.org/sco/ns#interactionOf
sco:interactionOf rdf:type owl:ObjectProperty ;
                  rdfs:domain sco:ServiceInteraction ;
                  rdfs:range sco:ServiceCapability ;
                  rdfs:comment "References to a service interaction that is part of a specific service capability." ;
                  rdfs:label "is interaction of" .


###  http://purl.org/sco/ns#manufacturerOf
sco:manufacturerOf rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:domain owl:Thing ;
                   rdfs:range sco:Device ;
                   rdfs:comment "Links the manufacturer to a specifc device." ;
                   rdfs:label "is manufacturer of" .


###  http://purl.org/sco/ns#outputFormatOf
sco:outputFormatOf rdf:type owl:ObjectProperty ;
                   rdfs:domain sco:Format ;
                   rdfs:range sco:DataModel ;
                   rdfs:comment "Is the expected outputformat from the device after executing the optionally transformation function." ;
                   rdfs:label "is output format of" .


###  http://purl.org/sco/ns#protocolStackOf
sco:protocolStackOf rdf:type owl:ObjectProperty ;
                    rdfs:domain sco:ProtocolStack ;
                    rdfs:range sco:ServiceInteraction ;
                    rdfs:comment "Is the protocol stack used for some interaction with a service capability." ;
                    rdfs:label "is protocolstack of" .


###  http://purl.org/sco/ns#serviceCapabilityOf
sco:serviceCapabilityOf rdf:type owl:ObjectProperty ;
                        rdfs:domain sco:ServiceCapability ;
                        rdfs:range sco:Device ;
                        rdfs:comment "Is the service capability of a device." ;
                        rdfs:label "is servicecapability of" .


###  http://purl.org/sco/ns#transformationRuleOf
sco:transformationRuleOf rdf:type owl:ObjectProperty ;
                         rdfs:domain sco:TransformationRule ;
                         rdfs:range sco:DataModel ;
                         rdfs:comment "Is the transformation rule for some data model." ;
                         rdfs:label "is transformationrule of" .


###  http://purl.org/sco/ns#unitOf
sco:unitOf rdf:type owl:ObjectProperty ;
           rdfs:domain owl:Thing ;
