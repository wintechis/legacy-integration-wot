:TemperatureDecodingRule a sh:NodeShape ;
	sh:targetClass sosa:Observation ;
	sh:rule [
		a sh:JSRule ;    # This triple is optional
		sh:jsFunctionName "computeArea" ;
		sh:jsLibrary [ sh:jsLibraryURL "file://js/tempearture.js"^^xsd:anyURI;
                        sh:jsLibraryURL "file://js/utils.js"^^xsd:anyURI] ;
        sh:condition :temperatureSpecificationForBosch ;
    ] ;
    sh:property [
        sh:path :decodedValue ;
        sh:datatype xsd:integer ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
    ] .
    
	:clientEventCharacteristic 
    a :BluetoothCharacteristic, capability:ServiceCapability;
    capability:represents "Client Event";
    capability:hasProtocolStack capability:BluetoothLE;
    capability:hasInteraction [
        capability:hasAffordanceID "E95D93AF251D470AA062FA1922DFA9A8/E95D5404251D470AA062FA1922DFA9A8";
        a capability:WriteInteraction
    ];
    rdfs:comment "Allows the client to write events to inform the micro:bit of occurrences.";
    capability:hasField "Event_Types_And_Values: uint8[]".
	:clientRequirementsCharacteristic 
    a :BluetoothCharacteristic, capability:ServiceCapability;
    capability:represents "Client Requirements";
    capability:hasProtocolStack capability:BluetoothLE;
    capability:hasInteraction [
        capability:hasAffordanceID "E95D93AF251D470AA062FA1922DFA9A8/E95D23C4251D470AA062FA1922DFA9A8";
        a capability:WriteInteraction
    ];
    rdfs:comment "A variable length list of event data structures indicating the types of micro:bit events the client wishes to be informed of.";
    capability:hasField "Client_Requirements_Value: uint8[]".
	:microbitEventCharacteristic 
    a :BluetoothCharacteristic, capability:ServiceCapability;
    capability:represents "MicroBit Event";
    capability:hasProtocolStack capability:BluetoothLE;
    capability:hasInteraction [
        capability:hasAffordanceID "E95D93AF251D470AA062FA1922DFA9A8/E95D9775251D470AA062FA1922DFA9A8";
        a capability:ReadInteraction, capability:NotifyInteraction
    ];
    rdfs:comment "Allows notification of events from the micro:bit to the client.";
    capability:hasField "Event_Types_And_Values: uint8[]".

