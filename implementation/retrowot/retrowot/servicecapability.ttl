:temperatureCharacteristic 
    a :BluetoothCharacteristic, capability:ServiceCapability;
    capability:represents "Temperature";
    capability:hasProtocolStack capability:BluetoothLE;
    capability:hasInteraction [
        capability:hasAffordanceID "E95D6100251D470AA062FA1922DFA9A8/E95D9250251D470AA062FA1922DFA9A8";
        a capability:ReadInteraction, capability:NotifyInteraction
    ];
    rdfs:comment "Provides temperature data as a signed integer 8-bit value in degrees Celsius.";
    capability:hasField "temperature value: sint8";
    capability:hasDataModel [
		capability:hasInputFormat <http://qudt.org/vocab/unit/BYTE>;
		capability:hasOutputFormat <https://www.w3.org/2019/wot/json-schema#NumberSchema>; 
		capability:hasUnit  <http://qudt.org/vocab/unit/DEG_C>;
		rdfs:comment "Contains accelerometer measurements for X, Y and Z axes as 3 signed 16 bit values in that order and in little endian format. X, Y and Z values should be divided by 1000.";
		capability:hasDecodingRule [
			dct:conformsTo <https://freumi.inrupt.net/BinaryDataOntology.ttl#>;
            bdo:signed true;
            bdo:offset 0;
            bdo:bytelength 1;
            rdfs:comment "Signed integer 8 bit value in degrees celsius."
        ]
    ].
:characteristicAcceleration
	a :BluetoothCharacteristic, capability:ServiceCapability;
	capability:represents "Acceleration";
	rdfs:comment "Contains accelerometer measurements for X, Y and Z axes as 3 signed 16 bit values in that order";
    capability:hasProtocolStack capability:BluetoothLE;
	capability:hasInteraction [
		capability:hasAffordanceID "E95D0753251D470AA062FA1922DFA9A8/E95DCA4B251D470AA062FA1922DFA9A8";
		a capability:ReadInteraction, capability:SubscriptionCommand 
        ];
		
	capability:hasDataModel [
		capability:hasInputFormat <http://qudt.org/vocab/unit/BYTE>;
		capability:hasOutputFormat <https://www.w3.org/2019/wot/json-schema#ArraySchema>; 
		capability:hasUnit  <http://qudt.org/vocab/unit/G>;
		rdfs:comment "Contains accelerometer measurements for X, Y and Z axes as 3 signed 16 bit values in that order and in little endian format. X, Y and Z values should be divided by 1000.";
		capability:hasDecodingRule [
			dct:conformsTo <https://freumi.inrupt.net/BinaryDataOntology.ttl#>;
			bdo:pattern '{X}{Y}{Z}';
			bdo:variables [
				a bdo:Variable;
				rdfs:label "X";
				td:type "integer";
				bdo:bytelength 2;
				bdo:offset 0;
				bdo:scale 0.001;
				rdfs:comment "Acceleration in X-Direction"
			],
			[
				a bdo:Variable;
				rdfs:label "Y";
				td:type "integer";
				bdo:bytelength 2;
				bdo:offset 2;
				bdo:scale 0.001;
				rdfs:comment "Acceleration in X-Direction"
			],
			[
				a bdo:Variable;
				rdfs:label "Z";
				td:type "integer";
				bdo:bytelength 2;
				bdo:offset 4;
				bdo:scale 0.001;
				rdfs:comment "Acceleration in X-Direction"
			]
            ]
	].
:characteristicButtonBPress 
	a :BluetoothCharacteristic, capability:ServiceCapability;
	capability:represents "Button B State";
    capability:hasProtocolStack capability:BluetoothLE;
	rdfs:comment "State of Button B may be read on demand by a connected client or the client may subscribe to notifications of state change.";
	capability:hasInteraction [
		capability:hasAffordanceID "E95D9882251D470AA062FA1922DFA9A8/E95DDA91251D470AA062FA1922DFA9A8";
		a capability:ReadInteraction, capability:SubscriptionCommand
        ];
	capability:hasDataModel [
		capability:hasInputFormat <http://qudt.org/vocab/unit/BYTE>;
		capability:hasOutputFormat <https://www.w3.org/2019/wot/json-schema#NumberSchema> ; 
		capability:hasDecodingRule [
			dct:conformsTo <https://freumi.inrupt.net/BinaryDataOntology.ttl#>;
            bdo:bytelength 1;
            ];
        capability:hasEncodingRule [
            dct:conformsTo <http://www.w3.org/ns/shacl#>;
			dct:format "text/turtle";
			spdx:checkSum spdx:Checksum;
			prof:hasArtifact :TemperatureEncodingRule 
        ]
	].
:characteristicButtonAPress 
	a :BluetoothCharacteristic, capability:ServiceCapability;
	capability:represents "Button A State";
    capability:hasProtocolStack capability:BluetoothLE;
	rdfs:comment "State of Button A may be read on demand by a connected client or the client may subscribe to notifications of state change.";
	capability:hasInteraction [
		capability:hasAffordanceID "E95D9882251D470AA062FA1922DFA9A8/E95DDA90251D470AA062FA1922DFA9A8";
		a capability:ReadInteraction, capability:SubscriptionCommand
        ];
	capability:hasDataModel [
		capability:hasInputFormat <http://qudt.org/vocab/unit/BYTE>;
		capability:hasOutputFormat <https://www.w3.org/2019/wot/json-schema#NumberSchema>; ; 
		capability:hasDecodingRule [
			dct:conformsTo <https://freumi.inrupt.net/BinaryDataOntology.ttl#>;
            bdo:bytelength 1;
            ];
        capability:hasEncodingRule [
            dct:conformsTo <http://www.w3.org/ns/shacl#>;
			dct:format "text/turtle";
			spdx:checkSum spdx:Checksum;
			prof:hasArtifact :TemperatureEncodingRule 
        ]
