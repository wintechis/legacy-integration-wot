    capability:hasInteraction [ a capability:NotifyInteraction,
                capability:ReadInteraction ;
            capability:hasAffordanceID "E95D0753251D470AA062FA1922DFA9A8/E95DCA4B251D470AA062FA1922DFA9A8" ] ;
    capability:hasProtocolStack capability:BluetoothLE ;
    capability:represents "Accelerometer Data" ;
    rdfs:comment "Contains accelerometer measurements for X, Y and Z axes as 3 signed 16 bit values in little endian format. Values should be divided by 1000." .

:accelerometerPeriodCharacteristic a :BluetoothCharacteristic,
