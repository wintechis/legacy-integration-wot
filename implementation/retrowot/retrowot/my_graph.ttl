:TemperatureDecodingRule a sh:NodeShape ;
    sh:property [ sh:datatype xsd:integer ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path :decodedValue ] ;
    sh:rule [ a sh:JSRule ;
            sh:condition :temperatureSpecificationForBosch ;
            sh:jsFunctionName "computeArea" ;
            sh:jsLibrary [ sh:jsLibraryURL "file://js/tempearture.js"^^xsd:anyURI,
                        "file://js/utils.js"^^xsd:anyURI ] ] ;
    sh:targetClass sosa:Observation .

:WoTtoBLE :affordanceIDMapping :BluetoothToCapabilityMapping ;
